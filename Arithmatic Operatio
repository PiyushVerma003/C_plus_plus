#include <iostream>
int main () {
    float    x = 10;
    int y = 3;
    float z = x / y;
    int sum = x + y;
    int sub = x- y;
    int mul = x * y;
    std::cout << "Addition: " << sum << std::endl <<"Substraction: " << sub << std::endl <<"Multiplication: " << mul << std::endl << "Division: " << z << std::endl;
    return 0;
}
